<div class="grid_3 alpha omega">
  <% @politician.joint_initiate_bill_politicians[0...3].each_with_index do |tmp, i| -%>
    <div class="grid_1<%= i==0 ? " alpha" : i == 2 ? " omega" : "" %>">
      <% p = Politician.where(:_id => tmp[0]).first -%>
      <% if p && p.profile_photo? -%>
        <%= link_to image_tag(p.profile_photo.url(:square100), :style => "width:48px;height:48px;"), district_politician_path(p.id) %>
      <% else -%>
        <%= link_to image_tag("http://placehold.it/48x48"), '#'%>
      <% end -%>

      <div><%= p.nil? ? tmp[0] : p.name  %></div>
      <div><%= "#{(tmp[1].to_f/@politician.initiate_bills.count*100.0).round}%" %></div>
    </div>
  <% end -%>
</div>
