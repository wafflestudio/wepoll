<%= form_for [:admin,@notice] do |f| -%>
  <div class="table">
    <table class="listing form" cellpadding="0" cellspacing="0">
      <tr>
        <td><strong>제목</strong></td>
        <td><%= f.text_field :title%></td>
      </tr>
      <tr>
        <td><strong>내용</strong></td>
        <td><%= f.text_area :content%></td>
      </tr>
    </table>
  </div>

  <div class="actions">
    <%= submit_tag '확인' %>
  </div>
<% end -%>
