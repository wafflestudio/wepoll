<table class="listing" cellpadding="0" cellspacing="0">
  <tr>
    <th>이름</th>
    <th>생년월일</th>
    <th>지역구</th>
    <th>정당</th>
    <th>임기</th>
    <th>당선횟수</th>
  </tr>
<% @politicians.each do |p| -%>
  <tr>
    <td><%= p.name%></td>
    <td><%= p.birthday%></td>
    <td><%= p.district%></td>
    <td><%= p.party%></td>
    <td><%= (p.elections||[]).join(",")%></td>
    <td><%= p.election_count%></td>
  </tr>
<% end -%>
</table>

<%= paginate @politicians%>
